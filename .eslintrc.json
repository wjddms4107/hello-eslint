{
  "$schema": "https://json.schemastore.org/eslintrc",
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-var": "error",
    "no-unreachable": "error",
    "max-depth": ["error", { "max": 1 }]
  }
}
